{
  "name": "home-library",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "docker": "nodemon --exitcrash -L src/main.ts",
    "docker:dev": "nodemon -L src/main.ts",
    "docker:scan": "docker scan avshell/node2022q4 && docker scan avshell/postgres2022q4 ",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config=.src/ormconfig.ts",
    "migration:gen": "npm run typeorm migration:generate -- -n ./src/migrations",
    "migration:run": "npm run typeorm migration:run",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/ormconfig.ts",
    "migration:up": "node --loader ts-node ./node_modules/.bin/typeorm migration:run -d ./src/data-source.ts",
    "migration:down": "node --loader ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/data-source.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:withoutauth": "jest --config ./test/jest-e2e.json",
    "test:users": "jest \"./test/users.e2e.spec.ts\"",
    "test:tracks": "jest \"./test/tracks.e2e.spec.ts\"",
    "test:artists": "jest \"./test/artists.e2e.spec.ts\"",
    "test:albums": "jest \"./test/albums.e2e.spec.ts\"",
    "test:favorites": "jest \"./test/favorites.e2e.spec.ts\"",
    "test:auth": "cross-env TEST_MODE=auth jest --config ./test/jest-e2e.json --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "http-status-codes": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
